{
	"name": "form-alchemist",
	"version": "0.0.3",
	"description": "A React form builder with a drag-and-drop interface, featuring a Builder component to create customizable forms and a Render component to display and validate them dynamically.",
	"type": "module",
	"main": "dist/index.js",
	"module": "dist/index.es.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.es.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"rollup": "rollup -c --bundleConfigAsCjs"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"form",
		"builder",
		"react",
		"drag-and-drop",
		"dynamic",
		"validation"
	],
	"author": "Arjun KV",
	"license": "ISC",
	"dependencies": {
		"crypto-browserify": "^3.12.0",
		"nanoid": "^5.0.7"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@types/react": "^18.3.5",
		"antd": "^5.20.6",
		"react": "^18.3.1",
		"rollup": "^4.21.3",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-svg": "^2.0.0",
		"styled-components": "^6.1.13",
		"tslib": "^2.7.0",
		"typescript": "^5.6.2"
	},
	"peerDependencies": {
		"antd": "^5.0.0",
		"react": ">=16.8",
		"styled-components": "^6.0.0",
		"typescript": "^5.0.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/arjunkv145/form-alchemist.git"
	},
	"homepage": "https://github.com/arjunkv145/form-alchemist#readme",
	"bugs": {
		"url": "https://github.com/arjunkv145/form-alchemist/issues"
	},
	"engines": {
		"node": ">=14.0.0"
	}
}
